// Finite Verb Groups

Phase: FiniteVerbGroup
Input: Token
Options: control = appelt

//////////////////////////////////////////////
// MACROS

Macro: MODALS

(
{Token.string == "can"}
|
{Token.string == "could"}
|
{Token.string == "may"}
|
{Token.string == "might"}
|
{Token.string == "must"}
|
({Token.string == "ought"} {Token.string == "to"}) 
|
{Token.string == "should"}
|
{Token.string == "would"}
)

Macro: MODAL_NEG
(
 {Token.string == "cannot"}
 )
 

Macro: TO
(
({Token.string == "to"})?
)



Macro: ADVS
(
({Token.category == RB})[1,3]
)

Macro: DO
({Token.string == "do"}|{Token.string == "does"} | {Token.string == "did"})

Macro: NEGATION
(
 (DO)?
({Token.string == "not"}) |
({Token.string == "n't"}) |
({Token.string == "never"})
)

